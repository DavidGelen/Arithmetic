#!/bin/vi
## Git ignore files for Android Studio projects

# IDEA generated files
*.idea
*.iml
.idea/

# Gradle files
.gradle/
build/
/*/build/

# Generated files
bin/
gen/
out/

# Java class files
*.class

# Built application files
*.apk
*.ap_

# Files for the ART/Dalvik VM
*.dex

# Local configuration file (sdk path, etc)
local.properties

# Proguard folder generated by Eclipse
proguard/

# Log Files
*.log

# Cache files for MacOSX
.DS_Store

# Local property file to save the version
*-version.properties





